 ogr2ogr -f GeoJSON subway.geojson  PG:dbname=osm -sql "select count(1) from routes JOIN route_members ON routes.osm_id = route_members.osm_id JOIN roads ON roads.osm_id = route_members.member WHERE route='subway';"
